    "recipe": {
        "title": "Dill Weed Dip ",
        "ingredients": [
            "3/4 cup mayonnaise",
            "3/4 cup sour cream",
            "2 tablespoons dry minced onion",
            "2 teaspoons \"beau appetit\" or \"beau monde\" seasoning",
            "2 teaspoons dried dill weed"
        ],
        "instructions": [
            "Mix ingredients together. Chill. Serve with assorted raw vegetables."
        ],
        "picture_link": null
    }
