{
    "recipe": {
        "title": "Sesame-Miso Vinaigrette ",
        "ingredients": [
            "1 red Fresno chile, with seeds, finely chopped",
            "1/4 cup vegetable oil",
            "2 tablespoons fresh lime juice",
            "2 tablespoons white miso",
            "1 tablespoon reduced-sodium soy sauce",
            "1 tablespoon unseasoned rice vinegar",
            "1 teaspoon toasted sesame oil",
            "1 teaspoon toasted sesame seeds",
            "1/2 teaspoon grated peeled ginger"
        ],
        "instructions": [
            "Whisk all ingredients in a small bowl.",
            "DO AHEAD: Vinaigrette can be made 1 week ahead. Cover and chill."
        ],
        "picture_link": "https://assets.epicurious.com/photos/54acc32e6529d92b2c03fdf5/6:4/w_274%2Ch_169/51210220_sesame-miso-vinaigrette_1x1.jpg"
    }
}