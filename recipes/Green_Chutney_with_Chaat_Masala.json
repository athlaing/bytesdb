{
    "recipe": {
        "title": "Green Chutney with Chaat Masala ",
        "ingredients": [
            "1 (1 1/2)-inch piece ginger, peeled, chopped",
            "1 jalape\u00f1o, seeds removed, chopped",
            "1 1/2 cups cilantro leaves with tender stems",
            "1 1/2 cups mint leaves",
            "1/3 cup plain whole-milk Greek yogurt",
            "4 teaspoons fresh lemon juice",
            "4 teaspoons vegetable oil",
            "1 teaspoon chaat masala",
            "Kosher salt"
        ],
        "instructions": [
            "Pulse ginger, jalape\u00f1o, cilantro, mint, yogurt, lemon juice, oil, and chaat masala in a food processor until smooth; season with salt."
        ],
        "picture_link": "https://assets.epicurious.com/photos/59416e8912c27b1823808236/6:4/w_274%2Ch_169/green-chutney-with-chaat-masala.jpg"
    }
}