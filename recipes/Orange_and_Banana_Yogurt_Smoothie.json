{
    "recipe": {
        "title": "Orange and Banana Yogurt Smoothie ",
        "ingredients": [
            "1 large banana, peeled, sliced, frozen",
            "1 cup chilled orange juice",
            "1/2 cup plain nonfat yogurt",
            "4 ice cubes",
            "1 tablespoon honey"
        ],
        "instructions": [
            "Combine all ingredients in blender. Blend until smooth. Divide smoothie between 2 glasses and serve."
        ],
        "picture_link": "https://assets.epicurious.com/photos/57ea7cb0983afd6d50ac3dcc/6:4/w_274%2Ch_169/orange-and-banana-yogurt-smoothie.jpg"
    }
}