{
    "recipe": {
        "title": "The Widow's Touch ",
        "ingredients": [
            "1 cup ice cubes",
            "5 tablespoons Laird's Applejack",
            "3 tablespoons St-Germain (elderflower liqueur)",
            "1 tablespoon B\u00e9n\u00e9dictine liqueur",
            "2 dashes of angostura bitters"
        ],
        "instructions": [
            "Place ice cubes in cocktail shaker. Add remaining ingredients and stir 20 to 30 seconds. Strain into 2 chilled small coupe glasses and serve."
        ],
