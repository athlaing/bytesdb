{
    "recipe": {
        "title": "Sparkling Tarragon-Gin Lemonade ",
        "ingredients": [
            "10 large sprigs tarragon",
            "2 lemons, thinly sliced",
            "2 tablespoons sugar",
            "3/4 cup gin",
            "3/4 cup St-Germain (elderflower liqueur)",
            "1/3 cup fresh lemon juice",
            "1 12-ounce can club soda"
        ],
        "instructions": [
            "Muddle tarragon sprigs, lemon slices, and sugar in a large pitcher. Add gin, St-Germain, lemon juice, and club soda. Add ice and stir to combine. Serve over ice."
        ],
        "picture_link": "https://assets.epicurious.com/photos/54afdd942edcf2b41143a2c1/6:4/w_274%2Ch_169/51170270_tarragon-gin-lemonade_1x1.jpg",
