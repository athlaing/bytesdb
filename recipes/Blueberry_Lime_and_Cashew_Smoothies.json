    "recipe": {
        "title": "Blueberry, Lime, and Cashew Smoothies ",
        "ingredients": [
            "2 Medjool dates, pitted",
            "2 cups coconut water",
            "1 cup cashews",
            "1 cup fresh or frozen blueberries",
            "1/3 cup plain whole-milk Greek yogurt",
            "1 Tbsp. fresh lime juice",
            "Large pinch of kosher salt"
        ],
        "instructions": [
            "Pur\u00e9e dates, coconut water, cashews, blueberries, yogurt, lime juice, salt, and 2 cups ice in a blender until smooth."
        ],
